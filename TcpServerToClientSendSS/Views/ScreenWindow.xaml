<Window x:Class="TcpServerToClientSendSS.Views.ScreenWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TcpServerToClientSendSS.Views"
        mc:Ignorable="d"
        Title="ScreenWindow" Height="1080" Width="1920">
    <Grid>
        <StackPanel>
            <Image Source="{Binding Source,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"></Image>
        </StackPanel>
    </Grid>
</Window>
